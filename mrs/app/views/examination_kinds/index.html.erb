<h1>Listing examination_kinds</h1>

<table>
  <tr>
  </tr>

<% for examination_kind in @examination_kinds %>
  <tr>
    <td><%= examination_kind.name %></td>
    <td><%= link_to 'Show', examination_kind %></td>
    <td><%= link_to 'Edit', edit_examination_kind_path(examination_kind) %></td>
    <td><%= link_to 'Destroy', examination_kind, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New examination_kind', new_examination_kind_path %>


